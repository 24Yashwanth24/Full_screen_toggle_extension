{
    "manifest_version": 3,
    "name": "Fullscreen Notch Toggle",
    "version": "4.0",
    "description": "Toggle fullscreen mode using an injected notch on the page.",
    "permissions": [
      "scripting",
      "activeTab"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Fullscreen Notch"
    }
  }
  